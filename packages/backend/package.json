{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --files src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "sequelize-cli db:migrate --config sequelize.config.js --migrations-path src/db/migration --models-path src/db/models --seeders-path src/db/seeders",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --config sequelize.config.js --migrations-path src/db/migration",
    "db:migrate:drop": "sequelize-cli db:migrate:undo:all --config sequelize.config.js --migrations-path src/db/migration",
    "db:migrate:fresh": "sequelize-cli db:migrate:undo:all --config sequelize.config.js --migrations-path src/db/migration && npx sequelize-cli db:migrate --config sequelize.config.js --migrations-path src/db/migration",
    "db:migrate:refresh": "sequelize-cli db:drop --config sequelize.config.js && npx sequelize-cli db:create --config sequelize.config.js && npx sequelize-cli db:migrate --config sequelize.config.js --migrations-path src/db/migration",
    "db:seed": "sequelize-cli db:seed:all --config sequelize.config.js --seeders-path src/db/seeders",
    "db:seed:undo": "sequelize-cli db:seed:undo:all --config sequelize.config.js --seeders-path src/db/seeders",
    "make:migration": "sequelize-cli migration:generate --migrations-path src/db/migration --config sequelize.config.js --name",
    "make:seed": "sequelize-cli seed:generate --seeders-path src/db/seeders --config sequelize.config.js --name",
    "make:model": "sequelize-cli model:generate --models-path src/db/models --migrations-path src/db/migration --config sequelize.config.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.14.3",
    "next": "15.5.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.7",
    "shared-types": "workspace:*",
    "shared-utils": "workspace:*",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cookie": "^1.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}